@model IEnumerable<ProjetoInicio.Models.Contrato>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Proprietario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Inquilino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InicioPeriodoVigencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FimPeriodoVigencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Preco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.observacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiaPagamento)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Proprietario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inquilino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InicioPeriodoVigencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FimPeriodoVigencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Preco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.observacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiaPagamento)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
                @Html.ActionLink("Details", "Details", new { id=item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
